<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>start_date_registration</fullName>
    <active>true</active>
    <errorConditionFormula>DATEVALUE(Meeting_Start_Time__c) &lt;= Registration_End_Date__c</errorConditionFormula>
    <errorDisplayField>Meeting_Start_Time__c</errorDisplayField>
    <errorMessage>Please enter a start date greater than registration end date</errorMessage>
</ValidationRule>
