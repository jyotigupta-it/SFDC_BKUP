<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Order_Amount_Less_Than_5000</fullName>
    <active>true</active>
    <errorConditionFormula>Amount__c &lt; 5000</errorConditionFormula>
    <errorDisplayField>Amount__c</errorDisplayField>
    <errorMessage>Order Amount cannot be less than 5000</errorMessage>
</ValidationRule>
