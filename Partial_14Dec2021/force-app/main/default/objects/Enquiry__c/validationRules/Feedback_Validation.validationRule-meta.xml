<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Feedback_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL(Enquiry_Status__c , &quot;Closed&quot;),   ISBLANK(Team_Feedback__c) )</errorConditionFormula>
    <errorDisplayField>Team_Feedback__c</errorDisplayField>
    <errorMessage>Please fill the Team feedback field</errorMessage>
</ValidationRule>
