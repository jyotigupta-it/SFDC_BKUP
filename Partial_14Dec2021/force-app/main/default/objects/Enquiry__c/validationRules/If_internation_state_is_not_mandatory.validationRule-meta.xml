<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>If_internation_state_is_not_mandatory</fullName>
    <active>false</active>
    <errorConditionFormula>AND( NOT(ISPICKVAL(Enquiry_Type__c, &apos;International&apos;)),ISBLANK(TEXT(State__c) ) )</errorConditionFormula>
    <errorDisplayField>State__c</errorDisplayField>
    <errorMessage>State is mandatory</errorMessage>
</ValidationRule>
