<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Mobile_num_with_10_dig_or_above_10_digit</fullName>
    <active>true</active>
    <description>Mobile number with 10 digit or above 10 digit.</description>
    <errorConditionFormula>LEN(Mobile__c)  &lt;   10</errorConditionFormula>
    <errorDisplayField>Mobile__c</errorDisplayField>
    <errorMessage>Please enter 10 digit mobile number</errorMessage>
</ValidationRule>
