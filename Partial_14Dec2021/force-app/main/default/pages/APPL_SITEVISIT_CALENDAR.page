        Visualforce.remoting.Manager.invokeAction(
            '{!$RemoteAction.AmeyoCallController.callCustomer}',
            visitId, '{!$User.MobilePhone}', result[0].id,
            function(result, event){
            }, 
            {escape: true}
        );
    }
    else{
        alert("failed to create new Task " + result[0]);
    }
}
